   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "B站下载助手",
   "offline_enabled": true,
   "permissions": [ "tabs", "contextMenus", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.9",
   "web_accessible_resources": [ "*.*", "**/*.*" ]
