{
   "background": {
      "scripts": [ "lib.js", "bg.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "B站下载助手"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "lib.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
